  },
  "homepage": "https://github.com/Bouncey/freeCodeCamp-Elasticsearch#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.17.2",
    "chai": "^4.1.2",
    "chalk": "^1.1.3",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dotenv": "^4.0.0",
    "elasticsearch": "^13.0.1",
    "express": "^4.15.3",
